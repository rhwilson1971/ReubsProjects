<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wilmss.prayerrequestjournal"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.wilmss.prayerrequestjournal.PrayerListActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.wilmss.prayerrequestjournal.PrayerDetailActivity"
            android:label="@string/title_prayer_detail"
            android:parentActivityName=".PrayerListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".PrayerListActivity" />
        </activity>
        <provider android:name=".contentprovider.PrayerRequestContentProvider"
            android:authorities="com.wilmss.prayerrequestjournal.contentprovider">
            
            
        </provider>
    </application>

</manifest>
